<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd"> 
<web-app> 
	<display-name>CreateUserLogin</display-name> 
	<servlet> 
		<servlet-name>TestCreateUser</servlet-name> 
		<servlet-class>User</servlet-class> 
	</servlet>
	
	<servlet> 
		<servlet-name>TestLogin</servlet-name> 
		<servlet-class>Login</servlet-class> 
	</servlet>
	
	<servlet-mapping> 
		<servlet-name>TestCreateUser</servlet-name> 
		<url-pattern>/Testc</url-pattern> 
	</servlet-mapping> 
	
	<servlet-mapping> 
		<servlet-name>TestLogin</servlet-name> 
		<url-pattern>/Testl</url-pattern> 
	</servlet-mapping> 
	
</web-app>